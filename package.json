{
  "name": "@osn/previewer",
  "version": "0.7.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build:demo": "tsc && vite build",
    "build": "tsup --dts --format esm --clean --inject ./react-import.js ./src",
    "prepublishOnly": "npm run build",
    "release": "bumpp --tag --commit --push"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*",
    "./plugins": {
      "import": "./dist/plugins/index.mjs",
      "types": "./dist/plugins/index.d.ts"
    }
  },
  "dependencies": {
    "marked": "^4.0.17",
    "sanitize-html": "^2.7.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/marked": "^4.0.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/sanitize-html": "^2.6.2",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^1.3.0",
    "bumpp": "^8.2.1",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.5",
    "tsup": "^6.1.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
